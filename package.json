{
  "name": "example-create-react-app-express",
  "version": "1.0.0",
  "main": "server.js",
  "engines": {
    "npm": "8.3.0",
    "node": "17.3.1"
  },
  "scripts": {
    "heroku-postbuild": "npm install",
